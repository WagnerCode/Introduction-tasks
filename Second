             Console.WriteLine("Enter yout number, please: ");
             int number = int.Parse(Console.ReadLine());
             Console.WriteLine("Choose what power of number do you need - <1,10>");
             int choose = int.Parse(Console.ReadLine());
             switch (choose)
             {
                 case (10):
                     Console.WriteLine(Math.Pow(number, 10));
                     break;
                 case (9):
                     Console.WriteLine(Math.Pow(number, 9));
                     break;
                 case (8):
                     Console.WriteLine(Math.Pow(number, 8));
                     break;
                 case (7):
                     Console.WriteLine(Math.Pow(number, 7));
                     break;
                 case (6):
                     Console.WriteLine(Math.Pow(number, 6));
                     break;
                 case (5):
                     Console.WriteLine(Math.Pow(number, 5));
                     break;
                 case (4):
                     Console.WriteLine(Math.Pow(number, 4));
                     break;
                 case (3):
                     Console.WriteLine(Math.Pow(number, 3));
                     break;
                 case (2):
                     Console.WriteLine(Math.Pow(number, 2));
                     break;
                 case (1):
                     Console.WriteLine(Math.Pow(number, 1));
                     break;
             }

//7
            Console.Writeline("Enter height: ");
            int height = int.Parse(Console.ReadLine());
            Console.Writeline("Enter lenght: ");
            int lenght = int.Parse(Console.ReadLine());

            for (int i = 0; i < height; i++)
            {
                Console.WriteLine("\n");
                for (int j = 0; j < lenght; j++)
                {
                    Console.Write("*");
                }
            }
//8
//Треугольник
            int height = int.Parse(Console.ReadLine());

            for (int i = 0; i < height; i++)
            {
                for (int j = height; j > i; j--)
                {
                    Console.Write(" ");
                } 
                for (int j = 0; j <= i; j++)
                {
                    Console.Write("*");
                }
                
                Console.WriteLine();
            }



